{
    "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.24/MicrosoftTeams.schema.json",
    "manifestVersion": "1.24",
    "version": "1.0.2",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "Daiso_it",
        "websiteUrl": "https://www.example.com",
        "privacyUrl": "https://www.example.com/privacy",
        "termsOfUseUrl": "https://www.example.com/termsofuse"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "AsungOrgChart",
        "full": "OrgChartTabApp for AsungGroup"
    },
    "description": {
        "short": "아성 조직도앱",
        "full": "아성 그룹을 위한 조직도 앱입니다."
    },
    "accentColor": "#FFFFFF",
    "bots": [],
    "composeExtensions": [],
    "staticTabs": [
        {
            "entityId": "index0",
            "name": "Home",
            "contentUrl": "${{TAB_ENDPOINT}}/index.html",
            "websiteUrl": "${{TAB_ENDPOINT}}/index.html",
            "scopes": [
                "personal",
                "groupChat",
                "team"
            ]
        }
    ],
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": [
        "${{TAB_DOMAIN}}"
    ],
    "webApplicationInfo": {
        "id": "${{AAD_APP_CLIENT_ID}}",
        "resource": "api://${{APP_ID_URI_DOMAIN}}/${{AAD_APP_CLIENT_ID}}"
    }
}